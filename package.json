{
  "name": "anole",
  "version": "1.2.0",
  "description": "A chrome extension that transforms selected text",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify src/anole.js -t [ babelify --presets=env ] | uglifyjs -cm > ext/anole.min.js",
    "build-options": "browserify src/options.js -t [ babelify --presets=env ] | uglifyjs -cm > ext/options.min.js",
    "build-debug": "browserify src/anole.js -t [ babelify --presets=env ] > ext/anole.min.js",
    "build-options-debug": "browserify src/options.js -t [ babelify --presets=env ] > ext/options.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdreid/anole.git"
  },
  "author": "Trevor Reid <trevor.d.reid@gmail.com> (http://tdreid.github.io)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/tdreid/anole/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "cached-path-relative": ">=1.0.2",
    "cyrillic-to-translit-js": "^2.2.0",
    "fast-lorem-ipsum": "^1.2.0",
    "lodash": "^4.17.11",
    "uglify-js": "^3.4.1"
  },
  "homepage": "https://github.com/tdreid/anole#readme",
  "main": "anole.js",
  "dependencies": {},
  "keywords": [
    "text",
    "convert",
    "productivity",
    "selection",
    "chrome",
    "extension",
    "kamel",
    "kamelcase",
    "kebab",
    "kebabcase",
    "lower",
    "lowercase",
    "snake",
    "snakecase",
    "start",
    "startcase",
    "upper",
    "uppercase"
  ]
}
